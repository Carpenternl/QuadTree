<Window x:Class="QuadTreeDemoV2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:QuadTreeDemoV2"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <Label x:Name="FPS" Content="0"/>
            <Label x:Name="DrawCalls" Content="0"/>
            <CheckBox x:Name="ToggleQuadTree" Content="Toggle Quad Tree"/>
        </StackPanel>
        <local:ParticleWindow/>
    </Grid>
    
</Window>
